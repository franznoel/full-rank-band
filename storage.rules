rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /images/{allPaths=**} {
      allow read: if true; // Public read access to /images
    }
    match /{allPaths=**} {
      allow read: if request.auth != null; // Authenticated read access for other files
      allow write: if request.auth != null; // Authenticated write access
    }
  }
}
